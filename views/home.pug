extends layout

block content
  h1 Hackathon Starter
  p.lead A boilerplate for Node.js web applications.
  hr
  .row
    .panel.panel-default.arrival-panel
      .panel-body
        if user.profile.picture
          img.arrival-panel-img(src=user.profile.picture)
        else
          img.arrival-panel-img(src=user.gravatar(60))
